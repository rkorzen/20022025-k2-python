[run]
source = .
omit = 
    */migrations/*
    */tests/*
    */env/*
    */venv/*
    manage.py
    */settings.py
    */wsgi.py
    */asgi.py
    */apps.py
    */admin.py
    */urls.py

[report]
exclude_lines =
    pragma: no cover
    def __str__
    def __repr__
    if settings.DEBUG
    raise NotImplementedError
    if __name__ == .__main__.:
    pass
    raise ImportError 